{% extends 'baseAuthenticated.html.twig' %}

{% block title %}Primary Industry{% endblock %}

{% block body %}
    <div class="uk-container">
        <h3>Viewing MN CAREERwise data on {{ secondaryIndustry.name }}</h3>
        <p><a href="{{ path('salary_exploration_secondary_industry', { 'id': secondaryIndustry.primaryIndustry.id }) }}">Back to {{ secondaryIndustry.primaryIndustry.name }} exploration</a></p>
        <p>If the website below does not load, go directly to the <a href="{{ secondaryIndustry.url }}" target="_blank">Minnesota CAREERwise website</a>.</p>
        <div class="iframe-container uk-margin">
            <iframe src="{{ secondaryIndustry.url }}"></iframe>
        </div>
    </div>
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

