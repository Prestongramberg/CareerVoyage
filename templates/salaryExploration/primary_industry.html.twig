{% extends 'baseAuthenticated.html.twig' %}

{% block title %}Salary Exploration - Primary Industry{% endblock %}

{% block body %}

    <div class="uk-container">
        <div class="uk-section uk-section-muted uk-padding">
            <h3>Select a Primary Industry Career Field</h3>
            {% for primaryIndustry in primaryIndustries %}
