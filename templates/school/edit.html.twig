{% extends 'baseAuthenticated.html.twig' %}

{% form_theme form _self %}

{% block title %}Edit School{% endblock %}

{% block body %}
    {{ form_start(form, {attr: {'novalidate' : 'novalidate'}}) }}
    {% if form_errors(form) %}
        {{ form_errors(form) }}
    {% endif %}
    {{ form_row(form.name) }}
    {{ form_row(form.address) }}
    {{ form_row(form.email) }}
    {{ form_row(form.overviewAndBackground) }}
    <input type="submit" value="Update School">
    {{ form_end(form) }}
{% endblock %}
